.block
  .block-head
    %h2 Trails
  #trails.block-content
    - @photos.keys.each do |trail|
      %div.trail
        .trail-slideshow
          - @photos[trail].each do |image|
            %img{:src => "#{image.pic.url}"}
        %a{:href => "/trails/#{trail}"}
          = "#{trail.capitalize}"            
        %div.clearfix